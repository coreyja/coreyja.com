  "query": "\n            SELECT \n                t.tag_id,\n                t.name,\n                t.color,\n                t.created_at,\n                t.updated_at\n            FROM tags t\n            JOIN recipe_tags rt ON t.tag_id = rt.tag_id\n            WHERE rt.recipe_id = $1\n            ORDER BY t.name\n            ",
