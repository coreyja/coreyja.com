{
  "db_name": "PostgreSQL",
  "query": "\n            UPDATE jobs\n            SET LOCKED_BY = $1, LOCKED_AT = NOW()\n            WHERE job_id = (\n                SELECT job_id\n                FROM jobs\n                WHERE run_at <= NOW() AND locked_by IS NULL\n                ORDER BY priority DESC, created_at ASC\n                LIMIT 1\n                FOR UPDATE SKIP LOCKED\n            )\n            RETURNING job_id, name, payload, priority, run_at, created_at, context\n            ",
