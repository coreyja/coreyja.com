{
  "db_name": "PostgreSQL",
  "query": "\n                SELECT \n                    id,\n                    name,\n                    description,\n                    query,\n                    variables_schema,\n                    tags,\n                    created_at,\n                    updated_at,\n                    created_by\n                FROM linear_saved_queries\n                WHERE (name ILIKE $1 OR description ILIKE $1)\n                    AND tags && $2\n                ORDER BY created_at DESC\n                LIMIT $3\n                ",
