  "query": "\n            UPDATE discord_thread_metadata\n            SET participants =\n                CASE\n                    WHEN participants ? $1 THEN participants\n                    ELSE participants || to_jsonb($1::text)\n                END,\n                updated_at = NOW()\n            WHERE thread_id = $2\n            RETURNING *\n            ",
